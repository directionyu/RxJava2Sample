<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="MyMarginStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">@dimen/btn_top_5dp</item>
        <item name="android:layout_marginLeft">@dimen/btn_top_10dp</item>
        <item name="android:layout_marginRight">@dimen/btn_top_10dp</item>
    </style>

    <!--按钮样式-->
    <!--按钮样式-->
    <style name="MyBtnStyle" parent="@style/MyMarginStyle">
        <item name="android:textAllCaps">false</item>
    </style>

    <!--文本样式-->
    <style name="MyTextStyle" parent="@style/MyMarginStyle">
        <item name="android:textColor">@android:color/black</item>
    </style>

</resources>
